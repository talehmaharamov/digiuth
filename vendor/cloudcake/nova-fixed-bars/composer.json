{
  "name": "cloudcake/nova-fixed-bars",
  "description": "A Laravel Nova package to add a responsive and/or fixed sidebar and navigation bar to the Nova admin UI.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "gregoriohc/laravel-nova-theme-responsive": "~0.6"
  },
  "autoload": {
    "psr-4": {
      "NovaFixedBars\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "NovaFixedBars\\NovaFixedBarsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
